page {
  height: 100%;
}
.cates {
  height: 100%;
}
.cates .cates_container {
  /* less中使用calc的时候要注意 */
  height: calc( 100vh - 90rpx );
  display: flex;
}

.cates .cates_container .right_content {
  /* 子项 高度 100% flex */
  flex: 5;
}
.cates .cates_container .right_content .goods_group .goods_title {
  height: 80rpx;
  display: flex;
  justify-content: center;
  align-items: center;
}
.cates .cates_container .right_content  .goods_title .delimiter {
  color: rgb(250, 9, 9);
  padding: 0 10rpx;
}
.cates .cates_container .right_content  .goods_title .title {
  color: rgb(250, 9, 9);
}
.cates .cates_container .right_content  .goods_list {
  display: flex;
  flex-wrap: wrap;
}
.cates .cates_container .right_content  .goods_list navigator {
  width: 50%;
  text-align: center;
}
.cates .cates_container .right_content  .goods_list navigator image {
  width: 60%;
}